<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="goodwix.doctrine_json_odm.odm_auto_registrar" class="Goodwix\DoctrineJsonOdm\Service\ODMAutoRegistrar" public="true">
            <argument type="service" id="serializer"/>
            <argument type="service" id="annotation_reader"/>
            <argument type="collection"/>
        </service>
        <service id="goodwix.doctrine_json_odm.listener.entity_with_odm_pre_flush" class="Goodwix\DoctrineJsonOdm\EventListener\EntityWithOdmPreFlushListener" public="true">
            <argument type="service" id="property_accessor"/>
            <tag name="doctrine.event_listener" event="preFlush" connection="default"/>
        </service>
    </services>
</container>
